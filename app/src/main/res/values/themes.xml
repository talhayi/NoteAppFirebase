<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.FireBaseExample" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/primary_color</item>
    </style>

    <style name="Theme.FireBaseExample" parent="Base.Theme.FireBaseExample" />
    <style name="TitilliumWebBold">
        <item name="android:fontFamily">@font/titillium_web_bold</item>
    </style>

    <style name="TitilliumWebBlack">
        <item name="android:fontFamily">@font/titillium_web_black</item>
    </style>

    <style name="TitilliumWebLight">
        <item name="android:fontFamily">@font/titillium_web_light</item>
    </style>

    <style name="TitilliumWebRegular">
        <item name="android:fontFamily">@font/titillium_web_regular</item>
    </style>

    <style name="TitleStyle">
        <item name="android:fontFamily">@font/titillium_web_bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="SubTitleStyle">
        <item name="android:fontFamily">@font/titillium_web_bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="DescStyle">
        <item name="android:fontFamily">@font/titillium_web_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>

    <style name="TextStyle">
        <item name="android:fontFamily">@font/titillium_web_regular</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="ChipTextStyle">
        <item name="android:fontFamily">@font/titillium_web_regular</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize8Percent" parent="">
        <item name="cornerSize">8%</item>
    </style>
</resources>